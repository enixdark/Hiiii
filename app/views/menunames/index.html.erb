<%= content_for :right do %>


<p id="notice"><%= notice %></p>

<h1>Listing Menunames</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @menunames.each do |menuname| %>
      <tr>
        <td><%= link_to 'Show', menuname %></td>
        <td><%= link_to 'Edit', edit_menuname_path(menuname) %></td>
        <td><%= link_to 'Destroy', menuname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Menuname', new_menuname_path %>

<% end %>