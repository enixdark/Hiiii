<div class="breadscrumb">
    <div class="row">
    
    <div class="col-md-2">
    ddd
    </div>
    <div class="col-md-10">


<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table class="table table-striped table-bolder table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th>Email</th>
      <th>Role</th>
      <th>Level</th>
      <th>Login key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
        <td><%= user.level %></td>
        <td><%= user.login_key %></td>
        <td><%= link_to 'Show', users_view_path(user) %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>

</div>